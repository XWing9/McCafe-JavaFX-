<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>

<VBox alignment="CENTER" spacing="20.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="AppGUI.GuiController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <TilePane hgap="10" vgap="10" prefColumns="3">
        <Button text="Caffee"   onAction="#StartingOrder"/>
        <Button text="Machiato" onAction="#StartingOrder"/>
        <Button text="Tee"      onAction="#StartingOrder"/>
        <Button text="Latte"    onAction="#StartingOrder"/>
        <Button text="Button 5" onAction="#StartingOrder"/>
        <Button text="Button 6" onAction="#StartingOrder"/>
        <Button text="Button 7" onAction="#StartingOrder"/>
        <Button text="Button 8" onAction="#StartingOrder"/>
        <Button text="Button 9" onAction="#StartingOrder"/>
    </TilePane>

</VBox>
